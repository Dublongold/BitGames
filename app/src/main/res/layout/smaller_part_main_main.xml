            android:layout_height="wrap_content"
            android:columnCount="2">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/main_main_constraintLayout_wildSpin"
                android:layout_margin="10dp"
                android:layout_columnWeight="1"
                android:clickable="true"
                android:focusable="true"
                android:foreground="?android:selectableItemBackground">

                <ImageView
                    android:id="@+id/main_main_imageView_wildSpinPreview"
                    android:layout_width="150dp"
                    android:layout_height="200dp"
                    android:contentDescription="@string/wild_spin_preview_description"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/wild_spin_preview" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:paddingBottom="2dp"
                    android:fontFamily="@font/baloo_bhaina_2_semibold"
                    android:text="@string/main_wild_spin"
                    android:textColor="@color/white"
                    android:textSize="19sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/main_main_imageView_wildSpinPreview" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="32dp"
                    android:fontFamily="@font/baloo_bhaina_2"
                    android:text="@string/main_by_platipus"
                    android:textColor="#99FFFFFF"
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/main_main_imageView_wildSpinPreview" />

                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="10dp"
                    android:layout_marginTop="11dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/main_main_imageView_wildSpinPreview"
                    app:srcCompat="@drawable/five_stars"
                    tools:ignore="ContentDescription"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/main_main_constraintLayout_booksGiza"
                android:layout_margin="10dp"
                android:layout_columnWeight="1"
